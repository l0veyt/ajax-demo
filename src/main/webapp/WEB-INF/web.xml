<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>ajax-demo</display-name>
  <servlet>
    <servlet-name>ResponseXmlServlet</servlet-name>
    <servlet-class>com.xinlee.demo.ResponseXmlServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ResponseJsonServlet</servlet-name>
    <servlet-class>com.xinlee.demo.ResponseJsonServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ResponseXmlServlet</servlet-name>
    <url-pattern>/responseXmlServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ResponseJsonServlet</servlet-name>
    <url-pattern>/responseJsonServlet</url-pattern>
  </servlet-mapping>
</web-app>
